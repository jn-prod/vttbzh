{
  "name": "www.vtt.bzh",
  "version": "1.0.0",
  "description": "Breizh Mountain Bike Calendar",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{bootstrap/dist}/**/*\" src/app/lib -C",
    "prestart": "npm install",
    "jekyll": "bundle exec jekyll server",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "webpack": "cross-env NODE_ENV=development webpack --watch",
    "start": "concurrently \"npm run jekyll\" \"npm run webpack\"",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jn-prod/www-vtt-bzh.git"
  },
  "author": "nicolas jouanno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jn-prod/www-vtt-bzh/issues"
  },
  "homepage": "https://github.com/jn-prod/www-vtt-bzh#readme",
  "dependencies": {
    "angular": "^1.7.9",
    "angular-animate": "^1.7.9",
    "angular-aria": "^1.7.9",
    "angular-material": "^1.1.20",
    "angular-messages": "^1.7.9",
    "angular-resource": "^1.7.9",
    "angular-route": "^1.7.9",
    "bootstrap": "*",
    "concurrently": "^4.1.2",
    "jquery": "*",
    "popper": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "angular-mocks": "^1.7.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.0",
    "eslint-loader": "^2.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.0",
    "karma": "^4.4.1",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-webpack": "^4.0.2",
    "node-sass": "^4.13.0",
    "protractor": "^5.4.1",
    "sass-loader": "^7.3.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
